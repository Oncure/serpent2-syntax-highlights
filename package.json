{
  "name": "serpent-2-syntax-highlighting",
  "displayName": "Serpent 2 Syntax Highlighting",
  "publisher": "ColeFritsch",
  "version": "0.3.1",
  "engines": {
    "vscode": "^1.23.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "serpent",
        "aliases": [
          "Serpent",
          "serpent"
        ],
        "extensions": [
          ".se",
          ".sse"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "serpent",
        "scopeName": "source.serpent",
        "path": "./syntaxes/serpent.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "serpent",
        "path": "snippets/serpent.tmSnippets.json"
      }
    ]
  },
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/Oncure/serpent2-syntax-highlights.git"
  },
  "homepage": "https://github.com/Oncure/serpent2-syntax-highlights"
}
